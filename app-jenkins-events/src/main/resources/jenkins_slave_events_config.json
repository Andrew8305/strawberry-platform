{
  "configId": "jenkins_slave_events",
  "documentIdField": "id",
  "persistEvent": true,
  "enableVisualization": true,
  "enabled": true,
  "dataDefinitions": {
    "elasticsearchIndexDefinition": {
      "properties": {
        "id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "timeStamp": {
          "type": "date"
        },
        "displayName": {
          "type": "string",
          "index": "not_analyzed"
        },
        "availablePhysicalMemory": {
          "type": "long"
        },
        "totalPhysicalMemory": {
          "type": "long"
        },
        "idle": {
          "type": "boolean"
        },
        "diskSpace": {
          "type": "long"
        },
        "osType": {
          "type": "string",
          "index": "not_analyzed"
        },
        "slaveOffline": {
          "type": "long"
        },
        "offlineReason": {
          "type": "string"
        },
        "averageResponseTime": {
          "type": "long"
        }
      }
    },
    "database": {
      "mongo": {
        "upsertMode": true,
        "maxNumberOfDocsBatch": 100,
        "maxBatchSizeInBytes": 100000000
      }
    }
  },
  "dataTransformation": {
    "dataTransformerHookClass": "com.strawberry.apps.jenkins.JenkinsSlaveEventTransformer"
  },
  "notification": {
    "spel": {
      "notificationConfigs": [
        {
          "channelName": "offline_slaves_notification_channel",
          "durable": false,
          "webhookUrlProviderClass": "com.strawberry.apps.jenkins.SlackNotificationUrlProvider",
          "webHookDataTransformerClass": "com.strawberry.apps.jenkins.JenkinsSlaveEventSlackTransformer",
          "spelExpressionQuery": "['slaveOffline'] == 1"
        }
      ]
    }
  }
}
