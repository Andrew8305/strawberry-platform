{
  "configId": "jenkins_job_events",
  "documentIdField": "id",
  "persistEvent": true,
  "enableVisualization": true,
  "enabled": true,
  "dataDefinitions": {
    "elasticsearchIndexDefinition": {
      "properties": {
        "id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "jobType": {
          "type": "string",
          "index": "not_analyzed"
        },
        "jobName": {
          "type": "string"
        },
        "component": {
          "type": "string",
          "index": "not_analyzed"
        },
        "triggeredUserName": {
          "type": "string",
          "index": "not_analyzed"
        },
        "jobUrl": {
          "type": "string",
          "index": "not_analyzed"
        },
        "buildNumber": {
          "type": "long"
        },
        "startTime": {
          "type": "date"
        },
        "startTimeStamp": {
          "type": "long"
        },
        "duration": {
          "type": "long"
        },
        "totalCaseCount": {
          "type": "long"
        },
        "totalPassedCount": {
          "type": "long"
        },
        "totalFailedCount": {
          "type": "long"
        },
        "totalSkippedCount": {
          "type": "long"
        },
        "result": {
          "type": "string",
          "index": "not_analyzed"
        },
        "building": {
          "type": "boolean"
        },
        "endTimestamp": {
          "type": "long"
        },
        "lowerTimeWindow": {
          "type": "long"
        },
        "upperTimeWindow": {
          "type": "long"
        }
      }
    },
    "database": {
      "mongo": {
        "upsertMode": true,
        "maxNumberOfDocsBatch": 100,
        "maxBatchSizeInBytes": 1000
      }
    }
  },
  "dataTransformation": {
    "dataTransformerHookClass": "com.strawberry.apps.jenkins.JenkinsJobEventTransformer"
  },
  "notification": {
    "spel": {
      "notificationConfigs": [
        {
          "channelName": "failed_jobs_notification_channel",
          "durable": false,
          "webhookUrl": "https://hooks.slack.com/services/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",
          "webHookDataTransformerClass": "com.strawberry.apps.jenkins.JenkinsJobEventSlackTransformer",
          "spelExpressionQuery": "['result'] != null && ['result'].equals('SUCCESS') == false && ['lowerTimeWindow'] <= ['endTimestamp'] && ['upperTimeWindow'] > ['endTimestamp']"
        }
      ]
    }
  }
}
